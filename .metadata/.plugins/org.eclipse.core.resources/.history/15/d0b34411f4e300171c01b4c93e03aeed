import java.util.Scanner;

public class Map extends GameMain {
	

	//Create the map
	public void initialiseMap() {

		for(int x=0; x<5; x++) {
			for(int y=0; y<5; y++) 
			
			gameMap[x][y] = 0;
			
		}
					

		}
	

	//Printing map
	public void drawMap() {
		for(int x=0; x<5; x++) {
			for(int y=0; y<5; y++) 
				System.out.print(gameMap[x][y] + " ");
				System.out.println("");
			
			}
		
	}
	
	Scanner inputFromUser;
	
	//while loop player position != finish
}

	

	

	


